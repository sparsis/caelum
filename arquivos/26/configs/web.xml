<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	
	<display-name>fj26-notas-fiscais</display-name>
	
	<!--
		Servlet do JSF. Gerado pelo Eclipse 
	-->
	<servlet>
		<servlet-name>Faces Servlet</servlet-name>
		<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Faces Servlet</servlet-name>
		<url-pattern>*.xhtml</url-pattern><!-- Extensão do acesso ao JSF na URL -->
	</servlet-mapping>

	<!-- 
		(opcional)
		Configuração de onde o JSF salva o estado.
		Aqui é possível colocar a opção "server" (padrão)  ou "client"
	 -->
	<!-- <context-param>
		<param-name>javax.faces.STATE_SAVING_METHOD</param-name>
		<param-value>client</param-value>
	</context-param> -->

	<!-- 
		(opcional)
		Qual tema deve ser usado no PrimeFaces. O padrão é o "aristo".
		Pode usar outro tema instalando o JAR certo e apontando aqui.
	 -->
	<!-- <context-param>
		<param-name>primefaces.THEME</param-name>
		<param-value>aristo</param-value>
	</context-param> -->

	
</web-app>