<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.mule.tooling.core.contribution">
      <externalContribution
            contributionJar="mule-transport-sap-2.2.5.jar"
            contributionLibPathInMule="/lib"
            contributionLibs="mule-transport-sap-2.2.5.jar"
            contributionNamespace="http://www.mulesoft.org/schema/mule/sap"
            contributionNamespaceFile="http://www.mulesoft.org/schema/mule/sap/current/mule-sap.xsd"
            contributionNamespacePrefix="sap"
            contributionType="cloud-connector"
            icon="icons/small/sap-endpoint-24x16.png"
            minimumVersion="3.5.0"
            name="SAP"
            path="sap.EE.xml"
            version="2.2.5">
      </externalContribution>
   </extension>
   <extension
         point="org.mule.tooling.maven.populate">
      <contribution-jar
            jar="mule-transport-sap-2.2.5.jar">
      </contribution-jar>
   </extension>
   <extension
         point="org.mule.tooling.ui.modules.core.metadataresolver">
      <metadataresolver
            class="org.mule.tooling.ui.contribution.sap.resolver.SAPEndpointNodeMetaDataResolver"
            type="http://www.mulesoft.org/schema/mule/sap/endpoint">
      </metadataresolver>
   </extension>
   <extension
         point="org.mule.tooling.ui.modules.core.editorMetadataStrategy">
      <editorMetadataStrategy
            class="org.mule.tooling.ui.contribution.sap.metadata.SAPMetadataStrategy"
            id="http://www.mulesoft.org/schema/mule/sap/endpoint">
      </editorMetadataStrategy>
   </extension>
</plugin>
